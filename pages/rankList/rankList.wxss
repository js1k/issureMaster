.rank-list {
    width: 100%;
    height: 100%;
    /* padding:128rpx 32rpx 130rpx; */
    box-sizing: border-box;
    /* overflow: scroll; */
}

.scroll-container {
    width: 100%;
    height: auto;
    padding: 88rpx 32rpx 130rpx;
}

.rank-section {
    width: 100%;
    height: 100%;
    flex-grow: 1;
    box-sizing: border-box;
    flex-basis: 0;
}

.navigation {
    margin-bottom: 20rpx;
    /* margin-top:128rpx; */
}

.rank-rules {
    display: block;
    font-size: 22rpx;
    margin-bottom: 8rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    width: 100%;
    text-align: right;
    padding-top:15rpx;
}

.section-content {
    height: 100%;
}

.rank-content {
    background: linear-gradient(180deg, rgba(109, 161, 255, 1) 0%, rgba(101, 89, 255, 1) 100%);
    border-radius: 10px 10px 0px 0px;
    width: 100%;
    flex-grow: 1;
    flex-basis: 0;
    padding: 0 78rpx 0 38rpx;
}

.rank-medal {
    width: 64rpx;
    height: 64rpx;
}

.rank-colum {
    width: 100%;
    padding: 24rpx 0;
}

.colum-right {
    width: 100%;
    padding-bottom: 22rpx;
    border-bottom: 2rpx solid rgba(255, 253, 253, 0.2);
}

.colum-right::last-child {
    border-bottom: none;
}

.rank-header-box {
    width: 92rpx;
    height: 92rpx;
    border-radius: 50%;
    margin-right: 24rpx;
    background: linear-gradient(360deg, rgba(236, 53, 52, 1) 0%, rgba(246, 141, 90, 1) 46%, rgba(250, 199, 111, 1) 100%);
}

.rank-header {
    width: 84rpx;
    height: 84rpx;
    border-radius: 50%;
}

.main-name {
    font-size: 28rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    margin-bottom: 8rpx;
}

.rank-star-txt {
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    margin-top:8rpx;
}

.rank-star-icon {
    width: 88rpx;
    height: 40rpx;
    margin-right: 10rpx;
}

.common-star {
    width: 28rpx;
    height: 40rpx;
    margin-right: 4rpx;
}

.rank-medal-box {
    width: 64rpx;
    height: 64rpx;
    margin-right: 26rpx;
    color: white;
    font-size: 32rpx;
    margin-bottom:22rpx;
}
.order-num{
    width: 64rpx;
    height: 64rpx;
}

.none-data {
    text-align: center;
    margin: 240rpx 0 794rpx;
    font-size: 44rpx;
    font-weight: 400;
    color: rgba(255, 255, 255, 0.6);
}

.fixed-colm {
    width: 100%;
    position: fixed;
    bottom: 0;
    padding: 0 32rpx;
}

.fixed-inner {
    padding: 0 78rpx 0 38rpx;
    background: linear-gradient(180deg, rgba(109, 161, 255, 1) 0%, rgba(101, 89, 255, 1) 100%);
}

.integral-rank {
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
}

.none-border {
    border-bottom: none;
}

.rank-mask {
    width: 100%;
    height: 100%;
    padding: 40rpx 60rpx;
    text-align: center;
}

.rank-pic {
    width: 100%;
    height: auto;
}

.mask-close {
    width: 60rpx;
    height: 60rpx;
    margin: 50rpx 0 30rpx;
}
.user-star{
    width:24rpx;
    height:24rpx;
}