.swiper-item{
    width:100%;
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    -webkit-flex-direction: column;
    align-items: center;
    -webkit-align-items: center;
    justify-content: flex-start;
    -webkit-justify-content: flex-start;
}
.question-index-bar{
    /* flex-grow: 1;
    -webkit-flex-grow: 1 */
    height:96rpx;
}
.cards-tips{
    width:100%;
    height:50rpx;
    margin:20rpx 0;
}
.tips-txt{
    width:100%;
    color:white;
    font-size:24rpx;
    height:40rpx;
    opacity: 0;
    animation: tipsMove 6s linear infinite;
}
@keyframes tipsMove{
    0%{
        transform: translateX(700rpx);
        opacity: 1
    }
    100%{
        transform: translateX(-400rpx);
        opacity: 1
    }
}
@keyframes questionIn{
    0%{
        transform: translateY(20rpx);
        opacity: 0
    }
    76%{
        opacity: 0
    }
    77%{
        transform: translateY(20rpx);
        opacity: 0.5
    }
    100%{
        transform: translateY(0rpx);
        opacity: 1
    }
}
@keyframes readyIn{
    0%{
        transform: scale(3);
        opacity: 1
    }
    40%{
        transform: scale(1);
        opacity: 1
    }
    99%{
        opacity: 1
    }
    100%{
        opacity: 0
    }
}

@keyframes startIn{
    0%{
        opacity: 0
    }
    49%{

        opacity: 0
    }
    50%{
        transform: scale(3);
        opacity: 1
    }
    70%{
        transform: scale(1);
        opacity: 1
    }
    99%{
        transform: scale(1);
        opacity: 1
    }
    100%{
        transform: scale(1);
        opacity: 0
    }
}
@keyframes showCardBar{
    0% {
        opacity: 0;
        transform: translateY(30rpx)
    }
    71%{
        opacity: 0;
        transform: translateY(30rpx)
    }
    100%{
        opacity: 1;
        transform: translateY(0rpx)
    }
}
@keyframes moveDown{
    0%{
        transform: translateY(-40rpx);
        opacity:0
    }
    49%{
        transform: translateY(-40rpx);
        opacity:0
    }
    50%{
        transform: translateY(-40rpx);
        opacity:1
    }
    100%{
        transform: translateY(0rpx);
        opacity:1
    }
}
@keyframes btnBig{
    0%{
        opacity: 0
    }
    69%{
        opacity: 0
    }
    70%{
        opacity: 1;
        transform: scale(0.6)
    }
    100%{
        opacity: 1;
        transform: scale(1)
    }
}
@keyframes resultIn{
    from{
        transform: rotateY(0deg) scale(0.6);
    }
    to{
        transform: rotateY(720deg) scale(1);
    }
}
@keyframes reviewIn{
    from{
        transform: translate3D(-800rpx, 200rpx, 0) rotate(-30deg) scale(0.5)
    }
    to{
        transform: translate3D(0, 0, 0) rotate(0deg) scale(1)
    }
}
@keyframes testIn{
    from{
        transform: translate3D(800rpx, 200rpx, 0) rotate(30deg) scale(0.5)
    }
    to{
        transform: translate3D(0, 0, 0) rotate(0deg) scale(1)
    }
}